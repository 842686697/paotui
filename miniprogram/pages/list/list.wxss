.backimg{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1000;
}
.float-{
  float: left;
}
.clearfix::after,.clearfix::before{
  content: "";
  display: table;
}
.clearfix::after{
  clear:both;
  overflow: hidden;
}
.list_box{
  margin: 30rpx 30rpx 0;
  height: 150rpx;
  width: 90%;
  background: #FFFAFA;
  border-radius: 10rpx;
  box-shadow: 0px 1px 5px darkgray;
  transition: all 0.3s;
  position: relative;
  opacity: 0.8;
}
.list_resizeBox{
  margin: 30rpx 30rpx 0;
  height: 900rpx;
  width: 90%;
  background: #FFFAFA;
  border-radius: 10rpx;
  box-shadow: 0px 1px 5px darkgray;
  transition: all 0.3s;
  position: relative;
  opacity: 0.8;
}
.list_content{
  height: 440rpx;
  width: 620rpx;
  position: absolute;
  left:30rpx;
  top: 160rpx;
  border-bottom: 4rpx lightgray solid;
}
.list_search{
  padding: 8rpx 20rpx 8rpx;
  margin: 20rpx 0 0 30rpx;
  height: 50rpx;
  width: 85%;
  border-radius: 10rpx;
  background:white; 
  font-size: 32rpx;
  color:black;
}
