page{
  background-color: rgba(211, 211, 211, 0.322)
}
.float-{
  float: left;
}
.clearfix::after,.clearfix::before{
  content: "";
  display: table;
}
.clearfix::after{
  clear:both;
  overflow: hidden;
}
.list_box{
  margin: 30rpx 30rpx 0;
  height: 150rpx;
  width: 90%;
  background: white;
  border-radius: 10rpx;
  box-shadow: 0px 1px 5px darkgray;
  transition: all 0.3s;
  position: relative;
}
.list_resizeBox{
  margin: 30rpx 30rpx 0;
  height: 900rpx;
  width: 90%;
  background: #D9FFFF;
  border-radius: 10rpx;
  box-shadow: 0px 1px 5px darkgray;
  transition: all 0.3s;
  position: relative;
}
.list_content{
  height: 450rpx;
  width: 650rpx;
  position: absolute;
  left:15rpx;
  top: 150rpx;
  border: 1px red solid;
  
}
.list_search{
  padding: 8rpx 20rpx 8rpx;
  margin: 20rpx 0 0 30rpx; 
  height: 50rpx;
  width: 85%;
  border-radius: 10rpx;
  background:white; 
  font-size: 30rpx;
}
.list_icon{
  margin: 15rpx 0 0 20rpx;
  height: 120rpx;
  width: 120rpx;
}
.list_message{
  margin-left: 10rpx;
  line-height: 75rpx;
  color:#8E8E8E;
  width: 200rpx;
}
.list_img{
  margin: 15rpx 15rpx 0 0;
  height: 120rpx;
  width: 120rpx;
}
.list_resizeImg{
  height: 230rpx;
  width: 230rpx;
  display: block
}
.list_imgs{
  position: absolute;
  left: 20rpx;
  top: 630rpx;
  height: 250rpx;
  width: 650rpx;
  border: 1px red solid
}
.list_add{
  border-radius: 50%;
  height: 150rpx;
  width: 150rpx;
  position:fixed;
  left:78%;
  top:85%;
  background: rgb(84, 142, 161);
  font-size: 40rpx;
  font-weight: bold;
  text-align: center;
  line-height: 150rpx;
  color:rgb(250, 228, 215);
  opacity: 0.8;
  box-shadow: 10rpx 10rpx 10rpx #888888;
}